{
  "proseWrap": "never",
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 120,
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-organize-attributes",
    "prettier-plugin-packagejson",
    "prettier-plugin-jsdoc"
  ],
  "overrides": [
    {
      "files": [
        "*.md"
      ],
      "options": {
        "printWidth": 120,
        "proseWrap": "always",
        "quoteProps": "as-needed",
        "semi": true,
        "singleQuote": false
      }
    }
  ]
}
